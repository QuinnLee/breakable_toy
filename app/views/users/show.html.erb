user#show
<p class="notice"><%= notice %></p>
<%= link_to "View Locations", locations_path %>
<%= pluralize(current_user.locations.size,'Destination') %>
<%if current_user.locations.size >0 %>
  <h1>Log</h1>
      <table>
        <tr>
          <th>Name</th>
        </tr>
      <% current_user.locations.each do |location| %>
        <tr>
          <td><%= link_to "#{location.name}", location %></td>
        </tr>
      <% end %>
  </table>
<% end %>